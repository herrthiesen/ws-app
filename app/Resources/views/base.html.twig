<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="">
        <meta name="author" content="">
        {% block stylesheets %}
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="{{asset('lib/css/bootstrap.css')}}">
        <!--<link rel="stylesheet" href="{{asset('lib/css/bootstrap.theme.min.css')}}">-->
        <link rel="stylesheet" href="{{asset('lib/css/style.css')}}">
        {% endblock %}
        <link rel="apple-touch-icon-precomposed" sizes="144x144" href="img/apple-touch-icon-144-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="114x114" href="img/apple-touch-icon-114-precomposed.png">
        <link rel="apple-touch-icon-precomposed" sizes="72x72" href="img/apple-touch-icon-72-precomposed.png">
        <link rel="apple-touch-icon-precomposed" href="img/apple-touch-icon-57-precomposed.png">
        <!-- Latest compiled and minified JavaScript -->
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        
        <script src="{{asset('lib/js/jquery-1.11.2.js')}}"></script>
        <script src="{{asset('lib/js/jquery-ui/jquery-ui.min.js')}}"></script>
        <script src="{{asset('lib/js/bootstrap.js')}}"></script>
        <script type="text/javascript">
            function OpenLink(theLink){
                window.location.href = theLink.href;
            }
        </script>
    </head>
    <body>
        <div class="container">
            <div class="row clearfix">
                <div class="col-md-12 column">
                    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                <span class="sr-only">Toggle navigation</span>
                                <a href="#" class="brackets">
                                    <span class="badge">1</span>[<img src="{{asset('lib/img/avatar.png')}}" width="25" class="img-circle user_avatar" id="user_avatar">]
                                </a>
                            </button>
                            <a class="navbar-brand" href="/partner/all"><img src="{{asset('lib/img/WIRSPARER-NLFY-418x60-white.png')}}" class="logo"/></a>
                        </div>
                        <div class="collapse navbar-collapse " id="bs-example-navbar-collapse-1">
                            <ul class="nav navbar-nav navbar-right">
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle brackets hidden-xs" data-toggle="dropdown">
                                        <span class="badge">1</span>[<img src="{{asset('lib/img/avatar.png')}}" width="25" class="img-circle user_avatar" id="user_avatar">]
                                    </a>
                                    <ul class="dropdown-menu">
                                        <li>
                                            <a href="/partner/all">Alle Partner</a>
                                        </li>
                                        <li>
                                            <a href="/partner/add">Partner anlegen</a>
                                        </li>
                                        <li>
                                            <a href="#">Something else here</a>
                                        </li>
                                        <li class="divider"></li>
                                        <li>
                                            <a href="{{path('logout')}}">Logout</a>
                                        </li>
                                    </ul>
                                </li>
                                <ul class="nav navbar-nav navbar-right hidden-sm hidden-md hidden-lg">
                                    <li>
                                        <a href="/partner/all">Alle Partner</a>
                                    </li>
                                    <li>
                                        <a href="/partner/add">Partner anlegen</a>
                                    </li>
                                    <li>
                                        <a href="#">Something else here</a>
                                    </li>
                                    <li class="divider"></li>
                                    <li>
                                        <a href="{{path('logout')}}">Logout</a>
                                    </li>
                                </ul>
                            </ul>
                        </div>
                    </nav>
                </div>
            </div><!--END BASE NAV ROW-->
            <div class="row clearfix">
                {% block body %}{% endblock %}
               
            </div>
        </div><!--END BASE CONTAINER-->
        {% block javascripts %}
        {% endblock %}
    </body>
</html>
